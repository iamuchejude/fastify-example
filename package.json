{
  "name": "fastify-example",
  "version": "1.0.0",
  "description": "Example web application with Fastify",
  "private": true,
  "main": "server.js",
  "scripts": {
    "prestart": "",
    "start": "node ./src/server.js",
    "start:dev": "./node_modules/.bin/nodemon --watch src ./src/server.js",
    "start:simple": "node ./src/server-simple.js",
    "postinstall": "",
    "cache:cleanup": "npm cache clean --force",
    "cache:verify": "npm cache verify",
    "debug": "node --inspect ./src/server.js",
    "debug:dev": "./node_modules/.bin/nodemon --debug --watch src ./src/server.js",
    "dependency:log": "npm list > ./temp/dependencies.log",
    "lint": "./node_modules/.bin/standard \"./src/**/*.js\" \"./test/**/*.test.js\"",
    "lint:log": "npm run lint > ./temp/lint-standard.log",
    "pretest": "",
    "test:coverage": "npm run test:unit -- --cov --coverage-report=html",
    "test:coverage:all": "npm run test:unit --  --cov",
    "test:unit": "tap test/*.test.js test/*/*.test.js",
    "test:unit:with-snapshot": "cross-env TAP_SNAPSHOT=1 npm run test:unit",
    "test": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "ejs": "^2.5.9",
    "fastify": "^1.3.0",
    "fastify-compress": "^0.6.0",
    "fastify-cookie": "^2.0.1",
    "fastify-env": "^0.4.0",
    "fastify-favicon": "^0.2.1",
    "fastify-session": "^1.0.0",
    "fastify-static": "^0.10.1",
    "fastify-webhook": "^0.1.0",
    "fast-json-stringify": "^1.2.1",
    "is-docker": "^1.1.0",
    "make-promises-safe": "^1.1.0",
    "point-of-view": "^1.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "nodemon": "^1.17.3",
    "semver": "^5.5.0",
    "standard": "^11.0.1",
    "simple-get": "^2.7.0",
    "tap": "^11.1.4"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=8.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartiniOnGitHub/fastify-example.git"
  },
  "keywords": [
    "node",
    "example",
    "fastify",
    "ejs",
    "es6",
    "es7",
    "es8"
  ],
  "author": "Sandro Martini <sandro.martini@gmail.com>",
  "license": "Apache-2.0"
}
